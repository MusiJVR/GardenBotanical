{
  "schemaVersion": 1,
  "id": "gardenbotanical",
  "version": "${version}",
  "name": "GardenBotanical",
  "description": "The mod adds 25 new colors that can be found all over the world! There are very rare flowers that can be found in the darkest corners of the world.",
  "authors": [
    "MouseJava",
    "redbad"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/gardenbotanical",
    "issues": "https://github.com/MusiJVR/GardenBotanical/issues"
  },
  "license": "MIT",
  "icon": "assets/gardenbotanical/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.gardenbotanical.GardenBotanical"
    ],
    "client": [
      "net.gardenbotanical.client.GardenBotanicalClient"
    ],
    "fabric-datagen": [
      "net.gardenbotanical.GardenBotanicalDataGenerator"
    ]
  },
  "mixins": [
    "gardenbotanical.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.modrinth": "https://modrinth.com/mod/gardenbotanical",
        "modmenu.source": "https://github.com/MusiJVR/GardenBotanical",
        "modmenu.discord": "https://discord.gg/xY8WJt7VGr"
      }
    }
  }
}
